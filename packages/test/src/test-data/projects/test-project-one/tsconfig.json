{
    "compilerOptions": {
        "target": "ESNEXT",
        "module": "ESNEXT",
        "lib": ["es2017", "es7", "es6", "dom"],
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "noEmit": false,
        "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
